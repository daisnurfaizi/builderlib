{
    "name": "daisnurfaizi/bubuilder",
    "description": "Membuat entity dan bulder pattern berdasarkan model database",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "bubuilder\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "require": {
        "php": "^7.1.3 || ^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "bubuilder\\BuilderServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "daisnurfaizi",
            "email": "dsnurfaizi@gmail.com"
        }
    ]
}
